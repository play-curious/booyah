{
  "name": "booyah",
  "version": "2.0.2",
  "description": "HTML5 game engine",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "testWatch": "jest --watch",
    "checkTypes": "tsc --noEmit",
    "checkLinting": "yarn eslint src/chip.ts src/running.ts",
    "checkFormatting": "yarn prettier --check ./src",
    "checkAll": "yarn checkTypes && yarn checkLinting && yarn checkFormatting",
    "prettier": "npx prettier ./src --write"
  },
  "license": "MIT",
  "dependencies": {
    "@types/underscore": "~1.11",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eventemitter3": "^5.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "underscore": "~1.13"
  },
  "repository": "https://github.com/play-curious/booyah.git",
  "author": "drpepper",
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/webpack-env": "^1.18.0"
  }
}
